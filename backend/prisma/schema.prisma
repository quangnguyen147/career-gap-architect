

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// MODELs

model Analysis {
  id                  String @id @default(uuid())
  cacheKey            String @unique
  resume              String
  jd                  String
  missingSkills       String[]
  learningSteps       String[]
  interviewQuestions  String[]
  createdAt           DateTime @default(now())

  @@index([cacheKey])
}
